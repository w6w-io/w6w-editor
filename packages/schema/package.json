{
    "name": "@w6w/schema",
    "version": "1.0.0-alpha.2",
    "description": "JSON Schema definitions for W6W (Workflow) components and configurations",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "private": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./workflow": {
            "types": "./dist/workflow.d.ts",
            "import": "./dist/workflow.js"
        },
        "./node": {
            "types": "./dist/node.d.ts",
            "import": "./dist/node.js"
        },
        "./edge": {
            "types": "./dist/edge.d.ts",
            "import": "./dist/edge.js"
        },
        "./variable": {
            "types": "./dist/variable.d.ts",
            "import": "./dist/variable.js"
        },
        "./execution-log": {
            "types": "./dist/execution-log.d.ts",
            "import": "./dist/execution-log.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "build": "pnpm clean && tsc",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "tsx": "^4.19.2",
        "typescript": "catalog:"
    }
}